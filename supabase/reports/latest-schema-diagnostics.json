  "rls_policies": [
    {
      "table": "profiles",
      "command": "SELECT",
      "definition": "(auth.uid() = user_id)",
      "policy_name": "Users can view their own profile."
    },
    {
      "table": "profiles",
      "command": "INSERT",
      "definition": null,
      "policy_name": "Users can insert their own profile."
    },
    {
      "table": "profiles",
      "command": "UPDATE",
      "definition": "(auth.uid() = user_id)",
      "policy_name": "Users can update their own profile."
    },
    {
      "table": "notifications",
      "command": "SELECT",
      "definition": "(auth.uid() = user_id)",
      "policy_name": "Los usuarios pueden ver sus propias notificaciones"
    },
    {
      "table": "notifications",
      "command": "UPDATE",
      "definition": "(auth.uid() = user_id)",
      "policy_name": "Los usuarios pueden marcar sus notificaciones como leídas"
    },
    {
      "table": "razprompts_entries",
      "command": "INSERT",
      "definition": null,
      "policy_name": "Los miembros del workspace pueden crear entradas de prompt"
    },
    {
      "table": "bavi_assets",
      "command": "SELECT",
      "definition": "true",
      "policy_name": "Permitir acceso de lectura pública a los activos"
    },
    {
      "table": "bavi_variants",
      "command": "SELECT",
      "definition": "true",
      "policy_name": "Permitir acceso de lectura pública a las variantes"
    },
    {
      "table": "workspaces",
      "command": "ALL",
      "definition": "public.is_workspace_member(id)",
      "policy_name": "Los usuarios pueden gestionar sus workspaces"
    },
    {
      "table": "workspace_members",
      "command": "SELECT",
      "definition": "public.is_workspace_member(workspace_id)",
      "policy_name": "Los miembros pueden ver a otros miembros"
    },
    {
      "table": "workspace_members",
      "command": "ALL",
      "definition": "(get_user_role_in_workspace(workspace_id) = 'owner'::text)",
      "policy_name": "Los propietarios pueden gestionar miembros"
    },
    {
      "table": "campaign_templates",
      "command": "ALL",
      "definition": "public.is_workspace_member(workspace_id)",
      "policy_name": "Los miembros del workspace pueden gestionar plantillas"
    },
    {
      "table": "bavi_assets",
      "command": "ALL",
      "definition": "public.is_workspace_member(workspace_id)",
      "policy_name": "Los miembros del workspace pueden gestionar activos"
    },
    {
      "table": "bavi_variants",
      "command": "ALL",
      "definition": "public.is_workspace_member(( SELECT bavi_assets.workspace_id\n   FROM bavi_assets\n  WHERE (bavi_assets.asset_id = bavi_variants.asset_id)))",
      "policy_name": "Los miembros del workspace pueden gestionar variantes"
    },
    {
      "table": "campaign_drafts",
      "command": "ALL",
      "definition": "is_workspace_member(workspace_id, 'member'::text)",
      "policy_name": "Los miembros del workspace pueden gestionar borradores"
    },
    {
      "table": "razprompts_entries",
      "command": "SELECT",
      "definition": "is_workspace_member(workspace_id, 'member'::text)",
      "policy_name": "Los miembros del workspace pueden ver las entradas de prompt"
    },
    {
      "table": "razprompts_entries",
      "command": "UPDATE",
      "definition": "is_workspace_member(workspace_id, 'member'::text)",
      "policy_name": "Los miembros del workspace pueden actualizar sus entradas de pr"
    },
    {
      "table": "razprompts_entries",
      "command": "DELETE",
      "definition": "is_workspace_member(workspace_id, 'member'::text)",
      "policy_name": "Los miembros del workspace pueden eliminar sus entradas de prom"
    },
    {
      "table": "cogniread_articles",
      "command": "SELECT",
      "definition": "true",
      "policy_name": "Public articles are viewable by everyone"
    },
    {
      "table": "commerce_orders",
      "command": "UPDATE",
      "definition": "(auth.role() = 'service_role'::text)",
      "policy_name": "Service can update orders"
    },
    {
      "table": "commerce_orders",
      "command": "DELETE",
      "definition": "false",
      "policy_name": "No one can delete orders"
    },
    {
      "table": "cogniread_articles",
      "command": "UPDATE",
      "definition": "((auth.role() = 'service_role'::text) OR (EXISTS ( SELECT 1\n   FROM workspace_members\n  WHERE ((workspace_members.user_id = auth.uid()) AND (workspace_members.role = 'owner'::text)))))",
      "policy_name": "Admin/Service can update articles"
    },
    {
      "table": "cogniread_articles",
      "command": "DELETE",
      "definition": "((auth.role() = 'service_role'::text) OR (EXISTS ( SELECT 1\n   FROM workspace_members\n  WHERE ((workspace_members.user_id = auth.uid()) AND (workspace_members.role = 'owner'::text)))))",
      "policy_name": "Admin/Service can delete articles"
    },
    {
      "table": "community_comments",
      "command": "SELECT",
      "definition": "true",
      "policy_name": "Comments are viewable by everyone"
    },
    {
      "table": "community_comments",
      "command": "UPDATE",
      "definition": "(auth.uid() = user_id)",
      "policy_name": "Authors can update their own comments"
    },
    {
      "table": "community_comments",
      "command": "DELETE",
      "definition": "(auth.uid() = user_id)",
      "policy_name": "Authors can delete their own comments"
    },
    {
      "table": "commerce_orders",
      "command": "SELECT",
      "definition": "(auth.uid() = user_id)",
      "policy_name": "Users can view their own orders"
    },
    {
      "table": "commerce_orders",
      "command": "INSERT",
      "definition": null,
      "policy_name": "Service and authenticated users can insert orders"
    },
    {
      "table": "community_comments",
      "command": "INSERT",
      "definition": null,
      "policy_name": "Authenticated users can insert their own comments"
    },
    {
      "table": "cogniread_articles",
      "command": "INSERT",
      "definition": null,
      "policy_name": "Admin/Service can insert articles"
    }
  ],
